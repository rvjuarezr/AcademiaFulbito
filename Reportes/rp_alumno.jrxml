	<parameter name="idAlumno" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select concat(a.Nombre," ",a.Apellido) as nombre_alumno, a.dni as dni_alumno, date_format(a.Fecha_nacimiento,'%Y-%m-%d') as fec_nac_alumno,
c.Nombre as categoria_alumno, concat(p.Nombre," ",p.Apellido) as nombre_padre, a.sexo as sexo_alumno, a.nacionalidad as nacionalidad_alumno
from alumno a
inner join padre p on a.ID_padre =p.ID_padre
inner join categoria c on a.ID_categoria = c.ID_categoria
where a.ID_alumno =$P{idAlumno};]]>
	</queryString>
