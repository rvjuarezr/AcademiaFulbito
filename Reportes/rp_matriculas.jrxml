	<parameter name="dniAlumno" class="java.lang.String"/>
	<queryString>
		<![CDATA[select concat(a.Nombre,',',a.Apellido) as nombres_alumno, c.Nombre as categoria_alumno,a.nacionalidad as nacionalidad_alumno,
a.Fecha_nacimiento as fec_nac_alumno, a.sexo as sexo_alumno, p.Telefono as telefono_alumno from bdacademiafulbito.matricula m
inner join bdacademiafulbito.alumno a on m.ID_alumno = a.ID_alumno
inner join bdacademiafulbito.categoria c on a.ID_categoria = c.ID_categoria
inner join bdacademiafulbito.padre p on a.ID_padre = p.ID_padre
where m.ID_matricula = $P{dniAlumno};]]>
	</queryString>
