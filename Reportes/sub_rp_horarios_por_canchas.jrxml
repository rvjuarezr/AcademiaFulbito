	<parameter name="sub_rp_ID_cancha" class="java.lang.String"/>
	<queryString>
		<![CDATA[select c.ID_cancha, h.Dia, h.Hora_inicio, h.Hora_fin, concat(p.nombre, ' ', p.apellido), c2.Nombre from bdacademiafulbito.cancha c
